    <script>


        let organizerCounter = 1;

        // switch to dark mode by os of user  
        if (window.matchMedia('(prefers-color-scheme: dark)').matches) {
            document.body.setAttribute('cds-theme', 'dark');
        }
              
        // add one Organizer
        function addOrganizer(event) {
            updateOrganizer(null);
            var input = document.createElement("input");
            input.placeholder="Max Mustermann";
            input.setAttribute("type", "text");
            input.setAttribute("onKeyUp", "updateOrganizer(event)");
            input.className = "clr-input organizerInput";
            input.name = `organizer[${organizerCounter}]`;
            document.getElementById("newOrganizer").appendChild(input);

            organizerCounter++;
        }

        // remove one Organizer
        function remOrganizer(event) {
            if (organizerCounter > 1) {
                const res = document.getElementsByName(`organizer[${organizerCounter-1}]`);
                res[0].parentNode.removeChild(res[0]);
                organizerCounter--;
                updateOrganizer(null);
            }
          
        }

        function updateOrganizer(event) {
            const organizerFields = document.querySelectorAll('.organizerInput');
            
            let organizerList = [];
            
            for (let field of organizerFields) {
                organizerList.push(field.value);
            }

            const disclaimer_text = document.getElementById("disclaimerText");
            disclaimer_text.innerHTML = `Please take note that you will need some time to fill out this survey. Your session will not be saved and will be lost when you close 
                the browser window. We <b>${organizerList.join(', ')}</b> hereby confirm that the survey responses we share are anonymized data. Anonymized data refers to
                information that does not identify an individual and cannot reasonably be used to determine their identity.  Anonymization requires the removal of the data
                subject’s name, address, and any other detail or combination of details that might support the identification of a data subject. Anonymized data does not identify
                a person, so it is not considered personal data under GDPR. Any identifying information provided in this form will only be used to provide information about this
                survey. It will remain confidential to the team of researchers at the University of Tartu (Estonia) who provide this service, and it will be deleted three months
                after the survey was created. If you have any further questions about this service, please contact Alexander Nolte (alexander.nolte@ut.ee) at the University of
                Tartu.`;

            const socialCapitalOrganizer = document.getElementById("socialCapitalOrganizer");
            socialCapitalOrganizer.innerHTML = `To what extent do you agree with the following statements about your relationship with ${organizerList.join(', ')}? (Strongly
                disagree, Somewhat disagree, Neither agree nor disagree, Somewhat agree, Strongly agree)`;
        }

        function updateHackathonTheme(event) {
            const motivationList = document.getElementById("motivationHackTheme");
            motivationList.innerHTML = event.currentTarget.value;
        }
        
        // Button to select all checkboxes for survey
        function selects(){
            var ele=document.getElementsByClassName('clr-checkbox');
            for(var i=0;i<ele.length;i++){
                if(ele[i].type=='checkbox')
                ele[i].checked=true;
            }
        }

        //Button to deselect all checkboxes for survey
        function deSelect(){
            var ele=document.getElementsByClassName('clr-checkbox');
            for(var i=0;i<ele.length;i++){
                if(ele[i].type=='checkbox')
                ele[i].checked=false;
            }
        }

        // look if disclaimer is checked
        document.querySelector('#senden').disabled = true;
        function disable() {
            document.querySelector('#senden').disabled = true;
        }
        function enable() {
            document.querySelector('#senden').disabled = false;
        }
    </script>
